        .token.operator, .token.entity, .token.url, .language-css .token.string, .style .token.string {
    color: #9a6e3a;
    background: black;
       }
        pre[class*="language-"] {
            color: rgb(247, 237, 237) !important; 
            background: black;
            text-shadow:none !important; 
            font-family: Consolas, Monaco, 'Andale Mono', 'Ubuntu Mono', monospace; /* Code font family */
            font-size: 1em; 
            text-align: left;
            white-space: pre;
            word-spacing: normal;
            word-break: normal;
            word-wrap: normal;
            line-height: 1.5; 
            text-shadow: none;
        }
        code[class*="language-"]{
            text-shadow: none;
        }
        .coding-editor pre code {
            color: inherit; 
            background: none; 
        }
        @media (max-width: 768px) {
  .navbar-toggler {
    display: block;
  }
        }
        
    </style>
</head>
<body>
    <h1>DSA Solutions</h1>
    
    <div class="solution">
        <h2>Python Solution</h2>
        <div class="coding-editor">
            <pre><code class="language-python"><%= pythonCode %></code></pre>
        </div>
    </div>

    <div class="solution">
        <h2>C++ Solution</h2>
        <div class="coding-editor">
            <pre><code class="language-cpp"><%= cppCode %></code></pre>
        </div>
    </div>

    <div class="solution" style="margin-bottom: 100px;">
        <h2>Java Solution</h2>
        <div class="coding-editor">
            <pre><code class="language-java"><%= javaCode %></code></pre>
        </div>
    </div>
    
